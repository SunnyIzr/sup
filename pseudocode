User:
  :username       :string
  :email          :string
  :password       :string
  
  :personal_rank  :integer (1-5)
  :mic_style      :integer (1-5)
  :play_style     :integer (1-5)
  
  has_many :time_slots, as: :availabilities
  
  
  
  
Platform:
  :name
  
Game:
  :name
  has_many :platforms
  
TimeSlots:
  :weekday
  :weedkay_name
  :start_time
  :end_time
  
  
Questions:
  Age or DOB?
  Check Box questions  